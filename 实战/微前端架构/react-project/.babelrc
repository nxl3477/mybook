{ 
  "presets": [ 
    "@babel/preset-react", 
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties", 
    "@babel/plugin-syntax-dynamic-import", 
    "@babel/plugin-transform-react-jsx-self"
  ]
}